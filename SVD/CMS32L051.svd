                </enumeratedValue>
                <enumeratedValue>
                  <name>Error</name>
                  <description>Parity error has occurred</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>

            <!-- RPERDIS: Disable RAM parity error reset -->
            <field>
              <name>RPERDIS</name>
